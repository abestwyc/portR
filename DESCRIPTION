Encoding: UTF-8
LazyData: true
Depends: R (>= 3.5.0),
    data.table
Imports:
    sandwich,
    dplyr,
    magrittr,
    plyr,
    e1071,
    bettermc
Suggests:
    htmlTable
RoxygenNote: 7.1.1
